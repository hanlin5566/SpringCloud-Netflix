spring.application.name=eureka-zuul
server.port=8200
eureka.client.serviceUrl.defaultZone=http://localhost:8761/eureka/
#zuul路由配置
#所有的api-a请求转发至eureka上的eureka-consumer服务
zuul.routes.api-a.path=/api-a/**
zuul.routes.api-a.serviceId=eureka-consumer
#所有的api-b请求转发至eureka上的eureka-consumer服务
zuul.routes.service-feign.path=/service-feign/**
zuul.routes.service-feign.serviceId=eureka-consumer-feign